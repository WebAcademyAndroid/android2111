
public class Main {

	public static void main(String[] args) {

		int salary = 100;

		if (salary < 500) {
			int a = 0;
			System.out.println("NEVER!");
		} else {
			int a = 0;
			System.out.println("YES!");
		}

		switch (salary) {
		case 100:
		case 2000:
			int a = 0;
			System.out.println("NEVER!");
			break;
		case 1000:
			int a = 0;
			System.out.println("YES!");
			break;
		default:
			int a = 0;
			System.out.println("DONT KNOW!");
			break;
		}
	}
}
